        "protocol": "tcp",
        "forwarding": "masq",
        "vip": "CCW-DICOMSERVER-VS",
        "ip": "!#VIP#!",
        "ports": "1230",
        "scheduler": "leastconn",
        "persistent": "off",
        "check_type": "external",
        "check_command": "c-Echo.sh",
