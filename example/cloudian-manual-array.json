{
  "method": "manual",
  "parameters": [
    {
        "name": "L7_VIP",
        "description": "Virtual IP Address",
        "type": "text",
        "method": "string",
        "default": ""
    },
    {
      "name": "VERSION",
      "description": "Version",
      "type": "text",
      "method": "string",
      "default": ""
    },
    {
        "name": "L7_RIPS",
        "description": "Cloudian Nodes",
        "type": "array",
        "method": "object",
        "default": "",
        "values": [
            ["Name", "rip"],
            ["IPAddress", "ip"]
        ]
    },
    {
        "name": "NAMESPACE",
        "description": "Namespace",
        "type": "text",
        "method": "string",
        "default": ""
    }
  ],

  "config": {
    "L7_vips": [
      {
        "mode": "tcp",
        "name": "cmc.!#NAMESPACE#!",
        "ip": "!#L7_VIP#!",
        "persistence": "ip",
        "persist_time": "30",
        "check_request": "/Cloudian/login.htm",
        "check_port": "8443",
        "check_type": "negotiate_http_head",
        "ports": "8888,8443",
        "rips": "!#L7_RIPS#!"
      },
      {
        "mode": "http",
        "name": "s3.!#NAMESPACE#!",
        "ip": "!#L7_VIP#!",
        "persistence": "none",
        "check_request": "/.healthCheck",
        "check_port": "",
        "check_type": "negotiate_http_head",
        "ports": "80",
        "rips": "!#L7_RIPS#!"
      },
      {
        "mode": "http",
        "name": "https.s3.!#NAMESPACE#!",
        "ip": "!#L7_VIP#!",
        "persistence": "none",
        "check_request": "/.healthCheck",
        "check_port": "",
        "check_type": "negotiate_https_head",
        "ports": "443",
        "rips": "!#L7_RIPS#!"
      },
      {
        "mode": "http",
        "name": "api.!#NAMESPACE#!",
        "ip": "!#L7_VIP#!",
        "persistence": "none",
        "check_username": "sysadmin",
        "check_port": "",
        "check_type": "negotiate_https_head",
        "ports": "19443",
        "rips": "!#L7_RIPS#!"
      },
      {
        "mode": "http",
        "name": "iam.!#NAMESPACE#!",
        "ip": "!#L7_VIP#!",
        "persistence": "none",
        "check_type": "connect",
        "ports": "16080",
        "rips": "!#L7_RIPS#!"
      },
      {
        "mode": "http",
        "name": "https.iam.!#NAMESPACE#!",
        "ip": "!#L7_VIP#!",
        "persistence": "none",
        "check_type": "connect",
        "ports": "16443",
        "rips": "!#L7_RIPS#!"
      }
    ],
    "L4_vips": [
    ]
  }
}